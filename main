#include <iostream>
#include <time.h>
#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

//Variables
int main(int argc, char** arc){
srand(time(NULL));
int col, fil;
int matrizA [col][fil];
int i,j, resp;
int min=1; 
int max=10;


                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
//Proceso
printf ("Declara tu matriz, que no sea mayor a 20 \n");
scanf("%d", &resp);
col=resp;
fil=resp;

for (i=0; i<col; i++){
	for (j=0; j<fil; j++){
		
		matrizA[i][j]= min + rand() / (RAND_MAX / (max - min + 1) + 1);
		
	}
}
for (i=0; i<col; i++){
	printf("\n");
	for (j=0; j<fil; j++){
		printf("%d",matrizA[i][j]);		
	}
	printf("\n");
}
									
	
	return 0;
}
